module c.sys.time;

alias time_t = long;

struct timeval
{
    time_t time_t;
    int suseconds_t;
}

struct tm
{
    int tm_sec;
    int tm_min;
    int tm_hour;
    int tm_mday;
    int tm_mon;
    int tm_year;
    int tm_wday;
    int tm_yday;
    int tm_isdst;
    int tm_gmtoff;
    char* tm_zone;
}

extern(C) int gettimeofday(timeval* tv, void* tz);
extern(C) tm* localtime_r(time_t* timep, tm* result);
